<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="@dimen/stroke_sickness"
                android:color="#A5A6A8"
                android:dashWidth="5dp"
                android:dashGap="5dp" />
        </shape>
    </item>
    <item
        android:end="-2dp"
        android:start="-2dp">
        <shape>
            <stroke
                android:width="@dimen/stroke_sickness"
                android:color="#A5A6A8" />
        </shape>
    </item>
</layer-list>